pkgname=bookmarks
pkgver=2.3.0
pkgrel=0
pkgdesc="Bookmarks app for bookmarking files from CoreApps family. "
arch="all"
url="https://gitlab.com/cubocore"
license="GPL-2.1"
depends="libcprime-dev qt5-qtbase"
depends_dev=""
makedepends="$depends_dev qt5-qtbase-dev"
source="$pkgname-$pkgver.tar.gz::https://gitlab.com/cubocore/$pkgname/-/archive/v$pkgver/$pkgname-v$pkgver.tar.gz"
builddir="$srcdir/$pkgname-v$pkgver"

build() {
	cd "$builddir"
	qmake-qt5
	make
}

check() {
	cd "$builddir"	
}

package() {
	cd "$builddir"
	INSTALL_ROOT="$pkgdir" make install
}
sha512sums="683983b89ec0ed0be31452e122de2aee63ecba24b325d0886323a4578b6e62743fca8bc417c8ca6cf255afb9345bc8fb17ebc6a4a88980057f06c8c3c9b24ebb  bookmarks-2.3.0.tar.gz"
