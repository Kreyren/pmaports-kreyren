pkgname=settings
pkgver=2.3.0
pkgrel=0
pkgdesc="Settings is for setup CoreApps settings from CoreApps family. "
arch="all"
url="https://gitlab.com/cubocore"
license="GPL-2.1"
depends="libcprime-dev qt5-qtbase"
depends_dev=""
makedepends="$depends_dev qt5-qtbase-dev"
source="$pkgname-$pkgver.tar.gz::https://gitlab.com/cubocore/$pkgname/-/archive/v$pkgver/$pkgname-v$pkgver.tar.gz"
builddir="$srcdir/$pkgname-v$pkgver"

build() {
	cd "$builddir"
	qmake-qt5
	make
}

check() {
	cd "$builddir"	
}

package() {
	cd "$builddir"
	INSTALL_ROOT="$pkgdir" make install
}
sha512sums="aad457e6fc7b5c6be98c31f0bfed9763396b89ec22b82e09d094bd0ed3cc56545917aa4e7211e8280e8e8f7f3ba36054321ab78d7f2117b3ebe8cdce0e763148  settings-2.3.0.tar.gz"
