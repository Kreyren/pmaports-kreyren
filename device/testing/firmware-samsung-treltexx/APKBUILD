pkgname=firmware-samsung-treltexx
pkgver=1
pkgrel=0
pkgdesc="Firmware for Galaxy Note 4 (treltexx)"
url="https://github.com/universal5433/proprietary_vendor_samsung"
arch="armv7"
license="proprietary"
options="!check !strip !archcheck !spdx !tracedeps pmb:cross-native"

# Source
_commit="5fe78ced24ecf0cff06b53d6d050ed59544572fa"
_url="$url/raw/$_commit"

_treltexx_wifi_files="
  bcmdhd_apsta.bin
	bcmdhd_ibss.bin
  bcmdhd_mfg.bin
  bcmdhd_sta.bin
  cred.conf
	nvram_mfg.txt
  nvram_mfg.txt_a0
	nvram_mfg.txt_murata
  nvram_net.txt
  nvram_net.txt_a0
  nvram_net.txt_murata
  olsrd.conf
"


for _i in $_treltexx_wifi_files; do
	source="$source $pkgname-$_commit-$_i::$_url/trelte-common/proprietary/system/vendor/etc/wifi/$_i";
done

package() {
	local file
	for file in $_treltexx_wifi_files; do
		install -Dm644 "$srcdir/$pkgname-$_commit-$file" \
		    "$pkgdir/lib/firmware/postmarketos/$file"
	done
}


